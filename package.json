{
  "name": "pmp-youtube-channel",
  "version": "1.0.0",
  "description": "Comprehensive YouTube channel automation system for PMP exam preparation content",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "generate-content": "node src/automation/scripts/content-generator.js",
    "generate-from-calendar": "node src/automation/scripts/content-generator.js calendar",
    "schedule-uploads": "node src/automation/scripts/upload-scheduler.js",
    "validate-env": "node -e \"require('./src/config/environment.js')\"",
    "validate-templates": "node src/automation/scripts/template-validator.js",
    "map-variables": "node src/automation/scripts/template-variable-mapper.js",
    "test-templates": "npm run validate-templates && npm run map-variables 1 tuesday dailyStudy",
    "generate-thumbnails": "node src/automation/scripts/thumbnail-generator.js",
    "generate-thumbnails-batch": "node src/automation/scripts/thumbnail-batch-processor.js",
    "validate-thumbnails": "node src/automation/scripts/thumbnail-batch-processor.js validate",
    "test-thumbnail-system": "node src/automation/scripts/test-thumbnail-system.js",
    "generate-metadata": "node src/automation/scripts/generate-metadata.js",
    "generate-metadata-all": "node src/automation/scripts/generate-metadata.js --all",
    "generate-metadata-week": "node src/automation/scripts/generate-metadata.js --week",
    "generate-metadata-examples": "node src/automation/scripts/metadata-example.js",
    "export-metadata-csv": "node src/automation/scripts/generate-metadata.js --export csv",
    "export-youtube-batch": "node src/automation/scripts/generate-metadata.js --export youtube",
    "calendar": "node src/automation/scripts/calendar-cli.js",
    "import-content": "node src/automation/scripts/content-import-wordpress.js",
    "validate-content": "node src/automation/scripts/content-import-validator.js",
    "import-content-safe": "npm run validate-content && npm run import-content",
    "test-import": "node src/automation/scripts/test-content-import.js",
    "upload-automation": "node src/automation/scripts/upload-automation.js",
    "playlist": "node src/automation/scripts/playlist-cli.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "docker:setup": "./scripts/docker-setup.sh",
    "docker:start": "./scripts/docker-helpers.sh start",
    "docker:stop": "./scripts/docker-helpers.sh stop",
    "docker:restart": "./scripts/docker-helpers.sh restart",
    "docker:status": "./scripts/docker-helpers.sh status",
    "docker:logs": "./scripts/docker-helpers.sh logs",
    "docker:health": "./scripts/docker-helpers.sh health",
    "docker:backup": "./scripts/docker-helpers.sh backup",
    "docker:dev": "./scripts/docker-helpers.sh dev",
    "docker:prod": "./scripts/docker-helpers.sh prod",
    "test:integration": "jest --testPathPattern=integration",
    "test:docker": "npm run docker:health && npm run test:integration"
  },
  "keywords": [
    "youtube",
    "pmp",
    "certification",
    "content-automation",
    "education",
    "project-management"
  ],
  "author": "PMP YouTube Channel Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "canvas": "^2.11.2",
    "commander": "^14.0.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.1",
    "googleapis": "^118.0.0",
    "handlebars": "^4.7.7",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "sqlite3": "^5.1.6",
    "mysql2": "^3.6.0",
    "redis": "^4.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/pmp-youtube-channel.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/pmp-youtube-channel/issues"
  },
  "homepage": "https://github.com/your-username/pmp-youtube-channel#readme"
}
