  "distribution": {
    "channels": {
      "youtube": {
        "enabled": true,
        "placement": ["video_descriptions", "community_posts", "end_screens"],
        "callToAction": "Download your free {title} - link in description!",
        "trackingParams": "utm_source=youtube&utm_medium=video&utm_campaign=lead_magnet"
      },
      "website": {
        "enabled": true,
        "placement": ["blog_posts", "landing_pages", "popup_forms"],
        "callToAction": "Get instant access to {title}",
        "trackingParams": "utm_source=website&utm_medium=form&utm_campaign=lead_magnet"
      },
      "email": {
        "enabled": true,
        "placement": ["signature", "newsletter", "welcome_sequence"],
        "callToAction": "Don't miss this free resource: {title}",
        "trackingParams": "utm_source=email&utm_medium=newsletter&utm_campaign=lead_magnet"
      },
      "social": {
        "enabled": true,
        "placement": ["posts", "stories", "bio_links"],
        "callToAction": "Free download: {title} - link in bio!",
        "trackingParams": "utm_source=social&utm_medium=post&utm_campaign=lead_magnet"
      }
    },
    "deliveryMethods": {
      "instant_download": {
        "enabled": true,
        "requireEmail": true,
        "downloadPage": "/download/{magnetId}",
        "expirationHours": 24
      },
      "email_delivery": {
        "enabled": true,
        "deliveryDelay": 0,
        "emailTemplate": "lead_magnet_delivery",
        "attachmentSize": "10MB"
      },
      "drip_sequence": {
        "enabled": false,
        "sequenceLength": 5,
        "daysBetween": 2,
        "emailTemplate": "drip_sequence"
      }
    },
    "trackingEnabled": true,
    "analyticsIntegration": {
      "googleAnalytics": true,
      "facebookPixel": true,
      "customEvents": true
    }
  },
  "emailCapture": {
    "provider": "mailchimp",
    "apiKey": "MAILCHIMP_API_KEY",
    "listId": "MAILCHIMP_LIST_ID",
    "requireEmail": true,
    "optionalFields": ["firstName", "lastName", "company", "experience"],
    "doubleOptIn": true,
    "welcomeSequence": {
      "enabled": true,
      "delay": 300,
      "template": "welcome_sequence"
    },
    "segmentation": {
      "enabled": true,
      "criteria": {
        "by_magnet": true,
        "by_audience": true,
        "by_source": true,
        "by_engagement": true
      }
    },
    "customFields": {
      "pmp_status": ["studying", "certified", "considering"],
      "experience_level": ["beginner", "intermediate", "advanced"],
      "study_week": ["1-3", "4-6", "7-9", "10-13", "completed"],
      "primary_interest": ["videos", "study_guide", "practice", "community"]
    }
  },
  "branding": {
    "colors": {
      "primary": "#2E86AB",
