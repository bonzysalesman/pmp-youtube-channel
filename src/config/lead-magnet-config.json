    }
  },
  "performance": {
    "tracking": {
      "downloads": true,
      "emailCaptures": true,
      "conversionRates": true,
      "sourceAttribution": true,
      "userJourney": true
    },
    "goals": {
      "downloadConversionRate": 0.15,
      "emailConversionRate": 0.30,
      "monthlyDownloads": 500,
      "monthlyEmailCaptures": 150
    },
    "alerts": {
      "lowDownloads": {
        "threshold": 50,
        "period": "week",
        "enabled": true
      },
      "lowConversion": {
        "threshold": 0.20,
        "period": "week",
        "enabled": true
      },
      "highBounceRate": {
        "threshold": 0.70,
        "period": "day",
        "enabled": true
      }
    },
    "reporting": {
      "frequency": "weekly",
      "recipients": ["marketing@example.com", "content@example.com"],
      "includeMetrics": [
        "total_downloads",
        "email_captures",
        "conversion_rates",
        "top_performers",
        "source_breakdown"
      ]
    }
  },
  "automation": {
    "regeneration": {
      "enabled": true,
      "schedule": "monthly",
      "triggers": ["template_update", "content_change", "performance_drop"]
    },
    "distribution": {
      "autoPost": {
        "enabled": false,
        "platforms": ["twitter", "linkedin"],
        "frequency": "weekly"
      },
      "emailSequences": {
        "enabled": true,
        "triggerOnDownload": true,
        "segmentByMagnet": true
      }
    },
    "optimization": {
      "abTesting": {
        "enabled": true,
        "testElements": ["titles", "descriptions", "call_to_action"],
        "sampleSize": 100,
        "confidenceLevel": 0.95
      },
      "personalizedRecommendations": {
        "enabled": false,
        "basedOn": ["download_history", "engagement_level", "study_progress"]
      }
    }
  },
  "integrations": {
    "emailMarketing": {
      "mailchimp": {
        "enabled": true,
        "apiKey": "MAILCHIMP_API_KEY",
        "listId": "MAILCHIMP_LIST_ID"
      },
      "convertkit": {
        "enabled": false,
        "apiKey": "CONVERTKIT_API_KEY",
        "formId": "CONVERTKIT_FORM_ID"
      }
    },
    "analytics": {
      "googleAnalytics": {
        "enabled": true,
        "trackingId": "GA_TRACKING_ID",
        "events": ["download", "email_capture", "form_view"]
      },
      "mixpanel": {
        "enabled": false,
        "projectToken": "MIXPANEL_TOKEN"
      }
    },
    "storage": {
      "aws": {
        "enabled": false,
        "bucket": "lead-magnets",
        "region": "us-east-1"
      },
      "local": {
        "enabled": true,
        "path": "./generated/lead-magnets"
      }
    }
  },
  "security": {
    "downloadLinks": {
      "expiration": 24,
      "maxDownloads": 3,
      "ipRestriction": false
    },
    "emailValidation": {
      "enabled": true,
      "blockDisposable": true,
      "blockInvalid": true
    },
    "rateLimiting": {
      "enabled": true,
      "maxDownloadsPerHour": 5,
      "maxDownloadsPerDay": 10
    }
  }
}